{
  "expo": {
    "name": "Shamiri-Journal",
    "slug": "Shamiri-Journal",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/3d-white-clip.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/3d-white-clip.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": 22,
      "package": "com.nigelomondi.smartjournal"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      "expo-camera",
      "expo-font"
      // [
      //   "expo-font",
      //   {
      //     "fonts": ["./assets/fonts/Lato-Regular.ttf", "./assets/fonts/Lato-Light.ttf", "./assets/fonts/Lato-Bold.ttf"]
      //   }
      // ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "37532a54-5dea-47b6-96ec-167e09475d1d"
      }
    },
    "owner": "nigelomondi",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/37532a54-5dea-47b6-96ec-167e09475d1d"
    }
  }
}
